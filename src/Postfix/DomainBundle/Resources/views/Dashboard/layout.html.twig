{% extends "PostfixDomainBundle::layout.html.twig" %}
 
{% block title %}
  Dashboard | {{ parent() }}
{% endblock %}

{% block body %}
<div id="dashboard" class="row">
	<div class="col-lg-12">
		{% if wo_breadcrumbs() is not empty %}
		<div class="row">
			<div class="col-lg-12">
				<div class="wo_breadcrumb visible-desktop">
					<div>
					{{ wo_render_breadcrumbs() }}
					</div>
				</div>
			</div>
		</div>
		{% endif %}
		{% block sub_body %}
		{% endblock  %}
	</div>
</div>
{% endblock %}

{% block custom_javascripts %}
	<!-- // <script src="{{ asset('js/cars.js') }}"></script> -->
{% endblock %}